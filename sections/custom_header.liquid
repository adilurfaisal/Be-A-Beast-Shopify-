<style>
  .header--container {
    -webkit-filter: drop-shadow(4px 4px 4px rgba(0,0,0,.2));
    -moz-filter: drop-shadow(4px 4px 4px rgba(0,0,0,.2));
    -ms-filter: drop-shadow(4px 4px 4px rgba(0,0,0,.2));
    filter: drop-shadow(4px 4px 4px rgba(0,0,0,.2));
    -webkit-backdrop-filter: blur(10px);
    -moz-backdrop-filter: blur(10px);
    -ms-backdrop-filter: blur(10px);
    backdrop-filter: blur(10px);
    
  }
  .header-wrapper {
    transition: all .5s cubic-bezier(.88,.03,.13,1.01);
  }
  
  .header-wrapper.hide--to-top {
    top: -80px;
  }
  
  .bg--blur {
    -webkit-clip-path: polygon(100% 0, 100% calc(100% - 15px), calc(100% - 15px) 100%, 15px 100%, 0 calc(100% - 15px), 0 0);
    -moz-clip-path: polygon(100% 0, 100% calc(100% - 15px), calc(100% - 15px) 100%, 15px 100%, 0 calc(100% - 15px), 0 0);
    -ms-clip-path: polygon(100% 0, 100% calc(100% - 15px), calc(100% - 15px) 100%, 15px 100%, 0 calc(100% - 15px), 0 0);
    clip-path: polygon(100% 0, 100% calc(100% - 15px), calc(100% - 15px) 100%, 15px 100%, 0 calc(100% - 15px), 0 0);
  }
  
  .header-wrapper {
    position: fixed;
    width: 100%;
    top: 0;
    background: transparent;
  }
  
  header-drawer {
    justify-self: start;
    width: 100%;
  }
  
  .header {
    transition: background 0.2s ease-in-out;
  }
  
  .shopify-section-header-sticky .header {
    background: rgba(255, 255, 255, 0.4);
  }

  @media screen and (min-width: 990px) {
    header-drawer {
      display: none;
    }
  }

  .menu-drawer-container {
    display: flex;
    width: 100%;
  }
  
  .menu-drawer-container .mobile--icon svg {
    width: 30px;
    height: 30px;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    width: 100%;
    display: inline-flex;
    flex-wrap: wrap;
    justify-content: center;
  }
  
  .list-menu.list-menu--inline li {
    margin-right: 20px;
  }
  
  .list-menu.list-menu--inline li:last-child {
    margin-right: 0;
  }

  summary.list-menu__item {
    padding-right: 2.7rem;
  }

  .list-menu__item {
    display: flex;
    align-items: center;
    font-size: 24px;
    text-transform: uppercase;
    line-height: 1.2em;
    font-family: var(--font-secondary-family);
    font-weight: 600;
/*     line-height: calc(1 + 0.3 / var(--font-body-scale)); */
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
    
    .header--wrapper {
      clip-path: polygon(100% 0, 100% calc(100% - 25px), calc(100% - 25px) 100%, 25px 100%, 0 calc(100% - 25px), 0 0);
    }
  }
</style>

{%- style -%}
  .section-header {
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-header {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }
{%- endstyle -%}

